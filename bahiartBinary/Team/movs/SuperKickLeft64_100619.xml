    <slot name="kickLeft1wait1" delta="0.041" />

    <slot name="kickLeft2" delta="0.020">
        <move id="&lleg3;" angle="89" />
        <move id="&lleg4;" angle="-1" />
        <move id="&lleg5;" angle="-40" />
    </slot>

    <slot name="kickLeft1wait2" delta="10.0" />

    <slot name="leftLegReward" delta="0.700">
        <move id="&rleg3;" angle="0" />
        <move id="&lleg2;" angle="10" />
        <move id="&lleg6;" angle="-10" />
        <move id="&lleg3;" angle="40" />
        <move id="&lleg4;" angle="-80" />
        <move id="&lleg5;" angle="40" />
    </slot>

    <slot name="leftLegDown" delta="1.000">

        <move id="&lleg3;" angle="0" />
        <move id="&lleg4;" angle="0" />
        <move id="&lleg5;" angle="0" />
    </slot>

    <slot name="shiftToLeft" delta="0.500">
        <move id="&lleg2;" angle="0" />
        <move id="&lleg6;" angle="0" />
        <move id="&rleg2;" angle="0" />
        <move id="&rleg6;" angle="0" />
    </slot>

</behavior>
