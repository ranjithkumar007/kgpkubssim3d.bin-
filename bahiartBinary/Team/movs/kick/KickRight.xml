        <move id="&rleg4;" angle="-14.5"    />
        <move id="&lleg5;" angle="12.5"    />
        <move id="&rleg5;" angle="12.5"    />

        <move id="&lleg1;" angle="0"    />
        <move id="&rleg1;" angle="0"    />
    </slot>

    <slot name="shiftBack" delta="0.350">
