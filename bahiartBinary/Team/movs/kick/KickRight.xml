    <slot name="kick1" delta="0.0814">

        <move id="&rleg3;" angle="88.7" />
        <move id="&rleg4;" angle="-71.4" />
        <move id="&rleg5;" angle="-9.04" />
    </slot>

    <slot name="kick2" delta="0.021">
        <move id="&rleg3;" angle="90" />
        <move id="&rleg4;" angle="0" />
        <move id="&rleg5;" angle="-60" />
    </slot>

	<slot delta="0.200" />

    <slot name="kickLegReward" delta="0.400">
        <move id="&rleg2;" angle="-10" />
