<behavior name="KickRight" type="SlotBehavior">
        
    <slot delta="0.170" />
        
    <slot name="shiftWeight" delta="0.400">
        <move id="&lleg2;" angle="-11" />
        <move id="&lleg6;" angle="11" />
        <move id="&rleg2;" angle="-11" />
        <move id="&rleg6;" angle="11" />

        <move id="&lleg3;" angle="8"    />
        <move id="&rleg3;" angle="8"    />
        <move id="&lleg4;" angle="-14.5"    />
        <move id="&rleg4;" angle="-14.5"    />
        <move id="&lleg5;" angle="12.5"    />
        <move id="&rleg5;" angle="12.5"    />

    </slot>

<!--<slot name="kickLegUp" delta="0.350"> -->
    <slot name="kickLegUp" delta="0.500">
        <move id="&rleg3;" angle="20" />
        <move id="&rleg4;" angle="-40" />
        <move id="&rleg5;" angle="20" />

        <move id="&lleg3;" angle="0"    />
        <move id="&lleg4;" angle="0"    />
        <move id="&lleg5;" angle="0"    />
    </slot>

