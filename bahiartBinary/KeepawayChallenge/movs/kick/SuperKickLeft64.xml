<behavior name="SuperKickLeft" type="ExpediteSlotBehavior" >

    <slot name="shiftToRight" delta="0.400"/>
    <slot name="shiftToRight" delta="0.400">
        <move id="&lleg2;" angle="11" />
        <move id="&lleg6;" angle="-11" />
        <move id="&rleg2;" angle="11" />
        <move id="&rleg6;" angle="-11" />
    </slot>

    <slot name="waitshiftToRight" delta="0.100" />

    <slot name="leftLegUp" delta="0.200">
        <move id="&lleg3;" angle="21" />
        <move id="&lleg4;" angle="-39" />
