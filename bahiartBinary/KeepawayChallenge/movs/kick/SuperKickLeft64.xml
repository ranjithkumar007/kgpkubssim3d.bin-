        <move id="&lleg5;" angle="75" />
    </slot>

    <slot name="kickLeft1" delta="0.020">
        <move id="&larm1;" angle="0" />
        <move id="&rarm1;" angle="0" />

        <move id="&rleg3;" angle="-20" />
        <move id="&rleg4;" angle="-20" />
        
        <move id="&lleg3;" angle="20" />
        <move id="&lleg4;" angle="-120" />
        <move id="&lleg5;" angle="58" />
    </slot>

    <slot name="kickLeft1wait1" delta="0.041" />

    <slot name="kickLeft2" delta="0.020">
        <move id="&lleg3;" angle="89" />
        <move id="&lleg4;" angle="-1" />
        <move id="&lleg5;" angle="-40" />
    </slot>

    <slot name="kickLeft1wait2" delta="0.150" />

    <slot name="getLow" delta="0.115">

        <move id="&lleg3;" angle="90"    />
        <move id="&rleg3;" angle="90"    />
        <move id="&lleg4;" angle="-120"    />
        <move id="&rleg4;" angle="-120"    />
